{
  "name": "lime-web",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "linklocal": "^2.8.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.1"
  },
  "dependencies": {
    "history": "^4.7.2",
    "jsonwebtoken": "^8.1.1",
    "lime-lib": "file:../lime-lib",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-markdown": "^3.2.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-svg": "^2.2.13",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build": "node-sass-chokidar src/scss -o src/css && react-scripts build && git --git-dir=build.git/ --work-tree=build/ add -A && git --git-dir=build.git/ --work-tree=build/ commit -am Build && git --git-dir=build.git/ --work-tree=build/ push deploy master && git --git-dir=build.git/ --work-tree=build/ push github master:built",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "node-sass-chokidar src/scss -o src/css && node-sass-chokidar src/scss -o src/css --watch"
  }
}
